    "i18n",
    "xliff"
  ],
  "author": "Daniel Schreiber",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/daniel-sc/ng-extract-i18n-merge/issues"
  },
  "homepage": "https://github.com/daniel-sc/ng-extract-i18n-merge#readme",
  "dependencies": {
    "@angular-devkit/architect": "^0.1301.0",
    "@angular-devkit/core": "^13.0.0",
    "@angular-devkit/schematics": "^13.0.0",
    "@schematics/angular": "^13.0.0",
    "xliff-simple-merge": "~0.6.0",
    "xml_normalize": "~0.8.1"
  },
  "devDependencies": {
    "@types/jest": "~27.4.0",
    "@types/xmldom": "^0.1.31",
    "jest": "~27.4.7",
    "ts-jest": "~27.1.3",
    "ts-node": "~10.4.0",
    "tslint": "~6.1.3",
    "typescript": "~4.5.5"
  },
  "files": [
    "{dist,builders,schematics}/**/*.{json,js,d.ts}",
    "src/schema.json",
    "builders.json"
  ]
