{
  "name": "ng-extract-i18n-merge",
  "version": "2.13.1",
  "description": "Extract and merge i18n xliff translation files for angular projects.",
  "main": "index.js",
  "builders": "builders.json",
  "schematics": "./schematics/collection.json",
  "ng-add": {
    "save": "devDependencies"
  },
  "ng-update": {
    "migrations": "./schematics/migrations/migrations.json"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test-coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/daniel-sc/ng-extract-i18n-merge.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "angular",
    "i18n",
    "xliff"
  ],
  "author": "daniel-schreiber@gmx.de",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/daniel-sc/ng-extract-i18n-merge/issues"
  },
  "homepage": "https://github.com/daniel-sc/ng-extract-i18n-merge#readme",
  "dependencies": {
    "@angular-devkit/architect": "^0.1301.0 || ^0.1401.0 || ^0.1501.0 || ^0.1601.0 || ^0.1700.0 || ^0.1800.0 || ^0.1900.0",
    "@angular-devkit/core": "^13.0.0 || ^14.0.0 || ^15.0.0 || ^16.0.0 || ^17.0.0 || ^18.0.0 || ^19.0.0",
    "@angular-devkit/schematics": "^13.0.0 || ^14.0.0 || ^15.0.0 || ^16.0.0 || ^17.0.0 || ^18.0.0 || ^19.0.0",
    "@schematics/angular": "^13.0.0 || ^14.0.0 || ^15.0.0 || ^16.0.0 || ^17.0.0 || ^18.0.0 || ^19.0.0",
    "xmldoc": "^1.1.3"
  },
  "peerDependencies": {
    "@angular-devkit/build-angular": "^13.0.0 || ^14.0.0 || ^15.0.0 || ^16.0.0 || ^17.0.0 || ^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@types/jest": "^28.1.8 || ^29.0.0",
    "@types/xmldoc": "^1.1.6",
    "jest": "^28.0.8 || ^29.0.0",
    "ts-jest": "^28.0.8 || ^29.0.0",
    "ts-node": ">=10.9.1",
    "typescript": ">=4.5.0"
  },
  "engines": {
    "node": ">=14.0.0"
  }
